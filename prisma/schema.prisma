datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")  // .env dosyasÄ±ndaki URL'yi kullanacak
}

generator client {
  provider = "prisma-client-js"
}

model Feedback {
  id        Int      @id @default(autoincrement())
  name      String   @default("")
  message   String
  createdAt DateTime @default(now())
}
